<script>
	var number=13;
    var hasFactors=false;
    for(var i=2;i<=Math.sqrt(number);i++)
    {
    		if(number%i==0){
            hasFactors=true;
            break;
            }
            
     }
     if(!hasFactors)
     console.log(number + " is prime");
     else
     console.log(number + " isn't prime");
     
            


</script>
